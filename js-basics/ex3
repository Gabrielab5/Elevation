const reservations = {
  Bob: { claimed: false },
  Ted: { claimed: true }
}

//const name = 'Ted'
//const name = 'Bob'
const name = 'Bo'

if (reservations[name]) {
  if (reservations[name].claimed) {
    console.log("Hmm, someone already claimed this reservation");
  } else {
    console.log("Welcome, " + name);
  }
} else {
  console.log("You have no reservation");
}
